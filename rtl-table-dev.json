{
  "id": "com.custom.rtl.table.dev",
  "version": "1.1.0",
  "name": "RTL Table Dev",
  "description": "Custom RTL table with sorting, formatting, and CSS class support",
  "newInstancePrefix": "RtlTableDev",
  "icon": "https://liorbenistyy.github.io/rtl-table-dev/icon-dev.png",
  "vendor": "CustomDev",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-custom-rtl-table-dev",
      "url": "https://liorbenistyy.github.io/rtl-table-dev/rtl-table-dev.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "dataBindings": {
    "mainBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "type": "dimension",
          "description": "Dimension columns"
        },
        {
          "id": "measures",
          "type": "mainStructureMember",
          "description": "Measure columns"
        }
      ]
    }
  },
  "properties": {
    "rtlEnabled": {
      "type": "boolean",
      "description": "Enable Right-to-Left Layout",
      "default": true
    },
    "cssClass": {
      "type": "string",
      "description":"CSS Class Name (applied to widget host)",
      "default": ""
    }
  },
  "methods": {},
  "events": {},
  "supportsMobile": true,
  "supportsExport": true,
  "supportsBookmark": true,
  "supportsViewportLoading": true
}

